     <div class="container is-fullhd">
<div class="tokenlogin" id="history" style="display: none;">
       <h1 >
           Token history:
       </h1>
  <div class="card tokenlogin" >
       <div class="card-content">
         <div class="media">
          <!-- <div class="media-left">
             <figure class="image is-48x48">
               <img class="is-rounded" src="https://cdn.discordapp.com/avatars/<%=  user.userid %>/<%= user.avatar %>.png?size=128" alt="Placeholder image">
             </figure>
           </div>-->
           <div class="media-content">
             <p class="title is-4" id="historyusername"></p>
             <p class="subtitle is-6" id="historytoken"></p>
           </div>

            <div class="buttons media-right">
              <button id="startfromhistory" class="button"><i class="far fa-play-circle"></i> Start</button>

          </div>
         </div>


</div>
       </div>
</div>
       <br></br>


       <script type="text/javascript">
         $("#startfromhistory").click(function() {
           selfbotapp(tokenlocal);
         })
       </script>



            <div class="container has-text-centered tokenlogin">

                <h1 class="title">
                    Token login
                </h1>

                <br>
                </br>
                <div class="field">
                    <div class="control">



                        <div class="control has-icons-left">
                            <input class="input is-primary is-inverted is-rounded" type="text" placeholder="Enter your token" style="color: hsl(0, 0%, 65%)" id="token" value="">
                            <script type="text/javascript">
                              if (localStorage.getItem("token")) {
                                var tokenlocal = localStorage.getItem("token")

                                $('#historyusername').html(localStorage.getItem("username"));
                                $('#historytoken').html(localStorage.getItem("token"));
                                $("#history").fadeIn();
                                $("tokenlogin").fadeIn();

                            } else {

                              $('#token').val("");
                              $("#history").fadeOut();
                             }
                            </script>
                            <span class="icon is-small is-left">
                                <i class="fas fa-neuter"></i>
                            </span>
                        </div>
                        <br><br />

                        <span class="is-centered">

                            <button class="button is-primary is-inverted is-centered" onclick="selfbotapp();" class="loginbtn">
                                <span class="icon">
                                    <i class="far fa-play-circle"></i>
                                </span>
                                <span>Start</span>
                            </button>
                        </span>


                    </div>
                </div>
            </div>



    <div class="invisible" id="connected" style="display: none;">
        <button onclick="logout();" class="button is-danger">Stop Selfbot</button>

        <h1>
            <center>Connected as: <p id="username"></p>
            </center>
        </h1>


        <p>
            <h2>Control panel:</h2>
        </p>
        <div class="grid-container">

            <div class="grid-item">
                Embed Image:<br><input type="text" id="iembedimage" value="<%= settings.embedimage %>">
                <br>
                <br>
                <img id="prembedimage" class="grid-img" src="<%= settings.embedimage %>" width="100px">
            </div>

            <div class="grid-item">
                Footer Image:<br><input type="text" id="ifooterimage" value="<%= settings.footerimage %>">
                <br>
                <br>
                <img id="prfooterimage" class="grid-img" src="<%= settings.footerimage %>" width="100px">
            </div>

            <div class="grid-item">
                Embed Color:<br><input id="iembedcolor" class="jscolor" value="<%= settings.embedcolor %>">
            </div>
            <div class="grid-item">
                Prefix:<br>
                <select class="selectmenu" id="iprefix">
                    <option value="/" class="option">/</option>
                    <option value="*" class="option">*</option>
                    <option value=">" class="option">></option>
                    <option value="<" class="option"><</option>
                    <option value="." class="option">.</option>
                    <option value="$" class="option">$</option>
                    <option value="!" class="option">!</option>
                    <option value=";" class="option">;</option>
                    <option value="," class="option">,</option>
                    <option value=":" class="option">:</option>

                    <option value="_" class="option">_</option>
                    <option value="-" class="option">-</option>
                </select>
            </div>
            <div class="grid-item">
                Afk Mode:
                <br><label class="switch">
                    <input type="checkbox" id="iafkmode" checked="<%= settings.afkmode %>">
                    <span class="slider round"></span>
                </label>
            </div>
            <div class="grid-item">
                Afk Message:<br><input type="text" id="iafkmessage" value="<%= settings.afkmessage %>">
            </div>
            <div class="grid-item">
              Logs Channel's Id:<br><input type="text" id="iafktransmitchannel" value="<%= settings.afktransmitchannel %>">
            </div>

            <div class="grid-item">
                Custom Status:

                <br><label class="switch">
                    <input type="checkbox" id="iactivitystatus" checked="<%= settings.activitystatus %>">
                    <span class="slider round"></span>
                </label>
            </div>

            <div class="grid-item">
                Status Message:<br><select class="selectmenu" id="istatustype">
                    <option value="STREAMING" class="option">Streaming</option>
                    <option value="WATCHING" class="option">Watching</option>
                    <option value="LISTENING" class="option">Listening</option>
                    <option value="PLAYING" class="option">Playing</option>
                </select>
                <input type="text" id="istatusmessage" value="<%= settings.statusmessage %>">
            </div>



            <div class="grid-item">
                Bot DMs (AdBlock):
                <br><label class="switch">
                    <input type="checkbox" id="iabstatus" checked="<%= settings.abstatus %>">
                    <span class="slider round"></span>
                </label>


            </div>

            <div class="grid-item">
                Nitro Sniper (autoclaimer):
                <br><label class="switch">
                    <input type="checkbox" id="insstatus" checked="<%= settings.nsstatus %>">
                    <span class="slider round"></span>
                </label>


            </div>


        </div>

        <br>
        <br>

        <div id="snackbartwo">Settings applied!</div>

        <button onclick="resetsettings();" class="button is-info">Load settings</button>
    <form id="settingsform" method="POST">

        <button class="button is-success" type="submit">Save settings</button>

        <div class="invisible" style="display:none">
            <p id="prefix" form="settingsform" name="settings[prefix]">/</p>
            <p id="embedcolor" form="settingsform" name="settings[embedcolor]">#654321</p>
            <p id="embedimage" form="settingsform" name="settings[embedimage]">https://noziro.now.sh/ressources/noziro_gif.gif</p>
            <p id="afktransmitchannel" form="settingsform" name="settings[afktransmitchannel]">681305768426995719</p>
            <p id="footerimage" form="settingsform" name="settings[footerimage]">https://noziro.now.sh/ressources/noziro_brown.png</p>
            <p id="afkmode" form="settingsform" name="settings[afkmode]">false</p>
            <p id="afkmessage" form="settingsform" name="settings[afkmessage]">Sorry dude, i'm afk</p>
            <p id="statusmessage" form="settingsform" name="settings[statusmessage]">Noziro Selfbot</p>
            <p id="activitystatus" form="settingsform" name="settings[activitystatus]">false</p>
            <p id="statustype" form="settingsform" name="settings[statustype]">STREAMING</p>
            <p id="abstatus" form="settingsform" name="settings[abstatus]">false</p>
            <p id="nsstatus" form="settingsform" name="settings[nsstatus]">false</p>
        </div>
</form>

<script>
$('#settingsform').submit(function(e){
    e.preventDefault();
    $.ajax({
        url: '/api/settings',
        type: 'post',
        data:$('#settingsform').serialize(),
        success:function(){
            // Whatever you want to do after the form is successfully submitted
        }
    });
});

</script>

